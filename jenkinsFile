  
pipeline {
    agent any 
    tools
   {
    maven 'Maven'
    jdk 'JDK8'
   }
   
   
   
   parameters {
        string(name: 'branch-name', defaultValue: 'master', description: 'which branch you want to checkout?')}
    stages {
       
        stage('Test') { 
            steps {
			    checkout scm
		
		}}
		
		stage('git checkout branch') { 
            steps {
			    bat "git checkout --track origin/${env.branch-name}" 
			    bat "git branch"
		
		}
		
			  
        }
        
        }
    }
