  
pipeline {
    agent any 
    tools
   {
    maven 'Maven'
    jdk 'JDK8'
   }
   
   
   
   parameters {
        string(name: 'branch-name', defaultValue: 'master', description: 'which branch you want to checkout?')}
    stages {
       
        stage('Test') { 
            steps {
			    checkout scm
			    script{
			    echo "my branch name is ${env.branch-name}"
                bat "git checkout branch ${env.branch-name}"
                bat "mvn clean" 
		}
            }
        }
        
        }
    }
