  
pipeline {
    agent any 
    tools
   {
    maven 'Maven'
    jdk 'JDK8'
   }
   
   parameters {
        string(name: 'branch-name', defaultValue: 'master', description: 'which branch you want to checkout?')}
    stages {
       
        stage('Test') { 
            steps {
			    checkout scm
                bat "git checkout branch ${branch-name}"
                bat "mvn clean" 
            }
        }
        
        }
    }
